{
    "version": 2,
    "builds": [
      {
        "src": "./backend/server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["backend/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      }
    ],
    "installCommand": "npm install --legacy-peer-deps"
  }
  